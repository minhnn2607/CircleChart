<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleChart">
        <attr name="sc_maxValue" format="integer"/>
        <attr name="sc_progressValue" format="integer"/>
        <attr name="sc_totalFontSize" format="dimension"/>
        <attr name="sc_totalFontColor" format="color"/>
        <attr name="sc_topFontSize" format="dimension"/>
        <attr name="sc_topFontColor" format="color"/>
        <attr name="sc_topText" format="string"/>
        <attr name="sc_targetText" format="string"/>
